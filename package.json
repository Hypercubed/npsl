{
  "name": "listr-cli",
  "version": "0.0.0",
  "main": "index.js",
  "author": "J. Harshbarger",
  "license": "MIT",
  "scripts": {
    "test": "xo && jest ./test",
    "test.update": "jest ./test -u",
    "version": "npsl version"
  },
  "bin": {
    "npsl": "./cmd.js"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "del-cli": "^0.2.0",
    "execa": "^0.5.0",
    "find-up": "^2.0.0",
    "jest": "^16.0.2",
    "js-yaml": "^3.6.1",
    "listr": "^0.6.1",
    "lodash": "^4.16.4",
    "meow": "^3.7.0",
    "prefix-matches": "^0.0.9",
    "xo": "^0.17.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "envs": [
      "node"
    ],
    "rules": {
      "no-console": "warn",
      "no-warning-comments": [
        1,
        {
          "terms": [
            "todo",
            "fixme",
            "xxx"
          ],
          "location": "start"
        }
      ]
    }
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "del-cli": "^0.2.0",
    "jest-cli": "^16.0.2"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
